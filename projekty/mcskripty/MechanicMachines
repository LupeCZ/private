 name of tool of player is "%{tamername}%":
  {usetamer} is "true"
  set {tamer.%location of block%} to true
  set {tameron.%location of block%} to false
  if {mechanicsmachinelocale} is "cs" or "cz": 
   replace all "<prefix>" with "%{mechprefix}%" in {messagecz.tamer.place}
   replace all "<tamername>" with "%{tamername}%" in {messagecz.tamer.place}
   send message "%{messagecz.tamer.place}%"
  else:
   replace all "<prefix>" with "%{mechprefix}%" in {messageen.tamer.place}
   replace all "<tamername>" with "%{tamername}%" in {messageen.tamer.place}
   send message "%{messageen.tamer.place}%" 
 if {tamerplacelogall} is "true":
