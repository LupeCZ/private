on rightclick on diamond block:
 if {tamer.%location of block%} is true:
  if {tameron.%location of block%} is true:
   set {tameron.%location of block%} to false 
   if {mechanicsmachinelocale} is "cs" or "cz":
    replace all "<prefix>" with "%{mechprefix}%" in {messagecz.tamer.off}
    replace all "<tamername>" with "%{tamername}%" in {messagecz.tamer.off}
    send message "%{messagecz.tamer.off}%" 
   else:
    replace all "<prefix>" with "%{mechprefix}%" in {messageen.tamer.off}
    replace all "<tamername>" with "%{tamername}%" in {messageen.tamer.off}
    send message "%{messageen.tamer.off}%"    
  else:
   if {mechanicsmachinelocale} is "cs" or "cz":
    replace all "<prefix>" with "%{mechprefix}%" in {messagecz.tamer.on}
    replace all "<tamername>" with "%{tamername}%" in {messagecz.tamer.on}
    send message "%{messagecz.tamer.on}%" 
   else:
    replace all "<prefix>" with "%{mechprefix}%" in {messageen.tamer.on}
    replace all "<tamername>" with "%{tamername}%" in {messageen.tamer.on}
    send message "%{messageen.tamer.on}%" 
   set {tameron.%location of block%} to true
   loop {tamermaxtimes} times:
    loop all entities in radius {tamerradius} around the event-block:
     if loop-entity is a wolf:
      {owner.%event-entity%} is not set
      loop all items in inventory of block over the event-block:
       if loop-item is bone: 
        tame loop-entity to player 
        remove 1 of loop-item from inventory of block over the event-block
      loop {tamercooldown} times:
       wait a second
     if loop-entity is a ocelot:
      {owner.%event-entity%} is not set
      loop all items in inventory of block over the event-block:
       if loop-item is fish: 
        tame loop-entity to player 
        remove 1 of loop-item from inventory of block over the event-block
      loop {tamercooldown} times:
       wait a second
     if loop-entity is a horse:
      {owner.%event-entity%} is not set
      loop all items in inventory of block over the event-block:
       if loop-item is apple: 
        tame loop-entity to player 
        remove 1 of loop-item from inventory of block over the event-block
      loop {tamercooldown} times:
       wait a second
	   
on tame:
 set {owner.%event-entity%} to tamer
	   
on command:
 if command is "%{mechanicshopcommand}%" or "/%{mechanicshopcommand}%":
  if player has permission "%{mechanicshoppermission}%":
   cancel the event 
   open chest with 1 rows named "%{mechanicshopname}%" to player
   replace all "<price>" with "%{mechanicshopprice5}%" in {mechanicshoplore5}
   replace all "<price>" with "%{mechanicshopprice4}%" in {mechanicshoplore4}
   replace all "<price>" with "%{mechanicshopprice3}%" in {mechanicshoplore3}
   replace all "<price>" with "%{mechanicshopprice2}%" in {mechanicshoplore2}
   replace all "<price>" with "%{mechanicshopprice1}%" in {mechanicshoplore1}
   format slot 0 of player with dispenser named "%{mechanicshopname1}%" with lore "%{mechanicshoplore1}%" to close then run "sudo %player% buy1"
   format slot 1 of player with dispenser named "%{mechanicshopname2}%" with lore "%{mechanicshoplore2}%" to close then run "sudo %player% buy2"
   format slot 2 of player with dispenser named "%{mechanicshopname3}%" with lore "%{mechanicshoplore3}%" to close then run "sudo %player% buy3"
   format slot 3 of player with dispenser named "%{mechanicshopname4}%" with lore "%{mechanicshoplore4}%" to close then run "sudo %player% buy4"
   format slot 4 of player with dispenser named "%{mechanicshopname5}%" with lore "%{mechanicshoplore5}%" to close then run "sudo %player% buy5"
command /buy1:
 trigger:
  if {mechanicshopenabled} is "true" or true:
   if balance of player is larger than {mechanicshopprice1}-1:
    replace all "<prefix>" with "%{mechprefix}%" in {messagecz.itembought}
    replace all "<prefix>" with "%{mechprefix}%" in {messageen.itembought}
    replace all "<prefix>" with "%{mechprefix}%" in {messagecz.nomoney}
    replace all "<prefix>" with "%{mechprefix}%" in {messageen.nomoney}
    if {mechanicshopitem1} is "tamer", "Tamer", "breaker", "Breaker", "AutoMiner", "Autominer", "autominer", "FoodGenerator","foodgenerator", "" , "Foodgenerator", "WoodCutter", "woodcutter" or "WoodCutter":
     if {mechanicshopitem1} is "tamer" or "Tamer":
      execute console command "give %player% diamond_block %{mechanicshopamount1}% name:%{tamername}% lore:%{tamerlore}%"
     if {mechanicshopitem1} is "breaker" or "Breaker":
      execute console command "give %player% dispenser %{mechanicshopamount1}% name:%{breakername}% lore:%{breakerlore}%"
     if {mechanicshopitem1} is "AutoMiner", "Autominer" or "autominer":
      execute console command "give %player% dispenser %{mechanicshopamount1}% name:%{autominername}% lore:%{autominerlore}%"
     if {mechanicshopitem1} is "FoodGenerator","foodgenerator" or "Foodgenerator":
      execute console command "give %player% dispenser %{mechanicshopamount1}% name:%{foodgeneratorname}% lore:%{foodgeneratorlore}%"
     if {mechanicshopitem1} is "WoodCutter", "Woodcutter" or "woodcutter":
      execute console command "give %player% diamond_block %{mechanicshopamount1}% name:%{woodcuttername}% lore:%{woodcutterlore}%"
     remove {mechanicshopprice1} from player's account
     if {mechanicsmachinelocale} is "cs" or "cz":
      send message "%{messagecz.itembought}%"
     else:
      send message "%{messageen.itembought}%"
    else:
     send message "<prefix> Problem in configuration of shop. Contact admin!"
   else:
    if {mechanicsmachinelocale} is "cs" or "cz":
     send message "%{messagecz.nomoney}%"
    else:
     send message "%{messageen.nomoney}%"
