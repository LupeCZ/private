   if balance of player is larger than {mechanicshopprice5}-1:
    replace all "<prefix>" with "%{mechprefix}%" in {messagecz.itembought}
    replace all "<prefix>" with "%{mechprefix}%" in {messageen.itembought}
    replace all "<prefix>" with "%{mechprefix}%" in {messagecz.nomoney}
    replace all "<prefix>" with "%{mechprefix}%" in {messageen.nomoney}
    if {mechanicshopitem4} is "tamer", "Tamer", "breaker", "Breaker", "AutoMiner", "Autominer", "autominer", "FoodGenerator","foodgenerator", "" , "Foodgenerator", "WoodCutter", "woodcutter" or "WoodCutter":
     if {mechanicshopitem5} is "WoodCutter", "Woodcutter" or "woodcutter":
      execute console command "give %player% diamond_block %{mechanicshopamount4}% name:%{tamername}% lore:%{tamerlore}%"
     if {mechanicshopitem5} is "breaker" or "Breaker":
      execute console command "give %player% dispenser %{mechanicshopamount5}% name:%{breakername}% lore:%{breakerlore}%"
     if {mechanicshopitem5} is "AutoMiner", "Autominer" or "autominer":
      execute console command "give %player% dispenser %{mechanicshopamount5}% name:%{autominername}% lore:%{autominerlore}%"
     if {mechanicshopitem5} is "FoodGenerator","foodgenerator" or "Foodgenerator":
      execute console command "give %player% dispenser %{mechanicshopamount5}% name:%{foodgeneratorname}% lore:%{foodgeneratorlore}%"
     if {mechanicshopitem5} is "tamer" or "Tamer":
      execute console command "give %player% dispenser %{mechanicshopamount5}% name:%{woodcuttername}% lore:%{woodcutterlore}%"
     remove {mechanicshopprice5} from player's account
     if {mechanicsmachinelocale} is "cs" or "cz":
      send message "%{messagecz.itembought}%"
     else:
      send message "%{messageen.itembought}%"
    else:
     send message "<prefix> Problem in configuration of shop. Contact admin!"
   else:
    if {mechanicsmachinelocale} is "cs" or "cz":
     send message "%{messagecz.nomoney}%"
    else:
     send message "%{messageen.nomoney}%"
	 
command /buy2:
 trigger:
  if {mechanicshopenabled} is "true" or true:
   if balance of player is larger than {mechanicshopprice2}-1:
    replace all "<prefix>" with "%{mechprefix}%" in {messagecz.itembought}
    replace all "<prefix>" with "%{mechprefix}%" in {messageen.itembought}
    replace all "<prefix>" with "%{mechprefix}%" in {messagecz.nomoney}
    replace all "<prefix>" with "%{mechprefix}%" in {messageen.nomoney}
    if {mechanicshopitem2} is "tamer", "Tamer", "breaker", "Breaker", "AutoMiner", "Autominer", "autominer", "FoodGenerator","foodgenerator", "" , "Foodgenerator", "WoodCutter", "woodcutter" or "WoodCutter":
     if {mechanicshopitem2} is "tamer" or "Tamer":
      execute console command "give %player% diamond_block %{mechanicshopamount2}% name:%{tamername}% lore:%{tamerlore}%"
     if {mechanicshopitem2} is "breaker" or "Breaker":
      execute console command "give %player% dispenser %{mechanicshopamount2}% name:%{breakername}% lore:%{breakerlore}%"
     if {mechanicshopitem2} is "AutoMiner", "Autominer" or "autominer":
      execute console command "give %player% dispenser %{mechanicshopamount2}% name:%{autominername}% lore:%{autominerlore}%"
     if {mechanicshopitem2} is "FoodGenerator","foodgenerator" or "Foodgenerator":
      execute console command "give %player% dispenser %{mechanicshopamount2}% name:%{foodgeneratorname}% lore:%{foodgeneratorlore}%"
     if {mechanicshopitem4} is "WoodCutter", "Woodcutter" or "woodcutter":
      execute console command "give %player% diamond_block %{mechanicshopamount4}% name:%{woodcuttername}% lore:%{woodcutterrlore}%"
     remove {mechanicshopprice2} from player's account 
     if {mechanicsmachinelocale} is "cs" or "cz":
      send message "%{messagecz.itembought}%"
     else:
      send message "%{messageen.itembought}%"
    else:
     send message "Problem in configuration of shop. Contact admin!"
   else:
    if {mechanicsmachinelocale} is "cs" or "cz":
     send message "%{messagecz.nomoney}%"
    else:
     send message "%{messageen.nomoney}%"
	 
on place iron block:
 name of tool of player is "WoodCutter":
  set {woodcutter.%location of block%} to true 
  set {woodcutteron.%location of block%} to false
  replace all "<prefix>" with "%{mechprefix}%" in {messageen.woodcutter.place}
  replace all "<prefix>" with "%{mechprefix}%" in {messagecz.woodcutter.place}
  replace all "<woodcuttername>" with "%{woodcuttername}%" in {messagecz.woodcutter.place}
  replace all "<woodcuttername>" with "%{woodcuttername}%" in {messageen.woodcutter.place}
  if {mechanicsmachinelocale} is "cs" or "cz":
   send message "%{messagecz.woodcutter.place}%"
  else:
   send message "%{messageen.woodcutter.place}%"
 if {woodcutterPlacelogall} is "true":
  if file "plugins/Mechanic Machines/logs/woodcutter/Place/all.txt" doesn't exist:
   create file "plugins/Mechanic Machines/logs/woodcutter/Place/all.txt"
   write "[%now%] %player% placed woodcutter at %location of player% in world %world%" at line {countfile}+1 to file "plugins/Mechanic Machines/logs/woodcutter/Place/all.txt"
  else:
   write "[%now%] %player% placed woodcutter at %location of player% in world %world%" at line {countfile}+1 to file "plugins/Mechanic Machines/logs/woodcutter/Place/all.txt"
 if {woodcutterPlacelogplayer} is "true":
  if file "plugins/Mechanic Machines/logs/woodcutter/Place/Players/%player%.txt" doesn't exist:
   set {countfile} to line count of file "plugins/Mechanic Machines/logs/woodcutter/Place/Players/%player%.txt"
   create file "plugins/Mechanic Machines/logs/woodcutter/Place/Players/%player%.txt"
   write "[%now%] %player% placed woodcutter at %location of player% in world %world%" at line {countfile}+1 to file "plugins/Mechanic Machines/logs/woodcutter/Place/Players/%player%.txt"
  else:
   set {countfile} to line count of file "plugins/Mechanic Machines/logs/woodcutter/Place/Players/%player%.txt"
   write "[%now%] %player% placed woodcutter at %location of player% in world %world%" at line {countfile}+1 to file "plugins/Mechanic Machines/logs/woodcutter/Place/Players/%player%.txt"

  
on rightclick on iron block:
 {woodcutter.%location of clicked block%} is true:
  replace all "<prefix>" with "%{mechprefix}%" in {messagecz.woodcutter.on}
  replace all "<prefix>" with "%{mechprefix}%" in {messagecz.woodcutter.off}
  replace all "<prefix>" with "%{mechprefix}%" in {messageen.woodcutter.on}
  replace all "<prefix>" with "%{mechprefix}%" in {messageen.woodcutter.off}
  replace all "<woodcuttername>" with "%{woodcuttername}%" in {messagecz.woodcutter.on}
  replace all "<woodcuttername>" with "%{woodcuttername}%" in {messagecz.woodcutter.off}
  replace all "<woodcuttername>" with "%{woodcuttername}%" in {messageen.woodcutter.on}
  replace all "<woodcuttername>" with "%{woodcuttername}%" in {messageen.woodcutter.off}
  if {woodcutteron.%location of clicked block%} is true:
   set {woodcutteron.%location of clicked block%} to false
   if {mechanicsmachinelocale} is "cs" or "cz":
    send message "%{messagecz.woodcutter.off}%"
   else:
    send message "%{messageen.woodcutter.off}%"
   stop trigger
  else:
   set {woodcutteron.%location of clicked block%} to true
   if {mechanicsmachinelocale} is "cs" or "cz":
    send message "%{messagecz.woodcutter.on}%"
   else:
    send message "%{messageen.woodcutter.on}%"
   stop trigger
   
every second:
 add 1 to {time}
 if {time} is {woodcuttercooldown}:
  loop all players:
   loop all blocks in radius {woodcutterradiusplayer} around the loop-player:
    if loop-block is iron block:
     {woodcutteron.%location of loop-block%} is true:
      loop all blocks in radius {woodcutterradiusblock} around the loop-block:
       if loop-block-2 is any log:
        add loop-block-2 to inventory of block over loop-block-1
        set block at loop-block-2 to air
        if block under the loop-block-2 is dirt or grass:
         set block at the loop-block-2 to sapling


  
