  send message "%{messagecz.tamer.break}%" 
 else:
  replace all "<tamername>" with "%{tamername}%" in {messageen.tamer.break}
  replace all "<prefix>" with "%{mechprefix}%" in {messageen.tamer.break}
  send message "%{messageen.tamer.break}%"
 if {tamerbreaklogall} is "true":
