command /givetm [<player>]:
 trigger:
  {usetamer} is "true"
  if player has permission "MechanicMachines.give.tamer":
   execute console command "give %player% diamond_block 1 name:%{tamername}% lore:%{tamerlore}%"
   if {mechanicsmachinelocale} is "cs" or "cz":
    replace all "<prefix>" with "%{mechprefix}%" in {messagecz.tamer.gived}
    replace all "<tamername>" with "%{tamername}%" in {messagecz.tamer.gived}
    send message "%{messagecz.tamer.gived}%"
   else:
    replace all "<prefix>" with "%{mechprefix}%" in {messageen.tamer.gived}
    replace all "<tamername>" with "%{tamername}%" in {messageen.tamer.gived}
    send message "%{messageen.tamer.gived}%"
  else:
   if {mechanicsmachinelocale} is "cs" or "cz":
    replace all "<prefix>" with "%{mechprefix}%" in {messagecz.tamer.givenoperms}
    replace all "<tamername>" with "%{tamername}%" in {messagecz.tamer.givenoperms}
    send message "%{messagecz.tamer.givenoperms}%" 
   else:
    replace all "<prefix>" with "%{mechprefix}%" in {messageen.tamer.givenoperms}
    replace all "<tamername>" with "%{tamername}%" in {messageen.tamer.givenoperms}
    send message "%{messageen.tamer.givenoperms}%" 
	
