 if {foodgenerator.drop.fish} is not "true":
  remove 350 from {dropss::*}
 if {foodgenerator.drop.gapple} is not "true":
  remove 322 from {dropss::*}
 if {foodgenerator.drop.gapple2} is not "true":
  remove enchanted golden apple from {dropss::*}
 if {foodgenerator.drop.potato} is not "true":
  remove 392 from {dropss::*}
 if {foodgenerator.drop.rabbit} is not "true":
  remove 412 from {dropss::*}
 if {foodgenerator.drop.bread} is not "true":
  remove 297 from {dropss::*}
 if {foodgenerator.drop.steak} is not "true":
  remove 364 from {dropss::*}
 if {foodgenerator.drop.chicken} is not "true":
  remove 366 from {dropss::*}
 if {foodgenerator.drop.baked_potato} is not "true":
  remove 393 from {dropss::*}
 if {foodgenerator.drop.pumpkinpie} is not "true":
  remove 400 from {dropss::*}
 if {foodgenerator.drop.rabbit_stew} is not "true":
  remove "413" from {dropss::*}
 if {foodgenerator.drop.cake} is not "true":
  remove "354" from {dropss::*}
 if {foodgenerator.drop.mushroom_stew} is not "true":
  remove "282" from {dropss::*}
