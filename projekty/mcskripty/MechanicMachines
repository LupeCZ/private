on rightclick on emerald block:
 if {fg.%location of block%} is true:
  if {fgon.%location of block%} is false:
   cancel the event 
   if {mechanicsmachinelocale} is "cs" or "cz":
    replace all "<prefix>" with "%{mechprefix}%" in {messagecz.foodgenerator.on}
    replace all "<foodgeneratorname>" with "%{foodgeneratorname}%" in {messagecz.foodgenerator.on}
    send message "%{messagecz.foodgenerator.on}%" to player
   else:
    replace all "<prefix>" with "%{mechprefix}%" in {messageen.foodgenerator.on}
    replace all "<foodgeneratorname>" with "%{foodgeneratorname}%" in {messageen.foodgenerator.on}
    send message "%{messageen.foodgenerator.on}%" to player
   set {fgon.%location of block%} to true 
   loop {foodgeneratormaxblockss} times:
    {fgon.%location of block%} is true 
    chance of 25%:
     {fgon.%location of block%} is true 
     drop random element out of {dropss::*} at event-block
     loop {foodgeneratorcooldown} times:
      wait a seconds
    chance of 25%:
     {fgon.%location of block%} is true 
     drop random element out of {dropss::*} left of event-block
     loop {foodgeneratorcooldown} times:
     loop {foodgeneratorcooldown} times:
      wait a seconds
    chance of 25%:
     {fgon.%location of block%} is true 
     drop random element out of {dropss::*} right of event-block
     loop {foodgeneratorcooldown} times:
      wait a seconds
    else:
     {fgon.%location of block%} is true 
     drop random element out of {dropss::*} behind the block over the block behind the event-block
     loop {foodgeneratorcooldown} times:
      wait a seconds 
  else:
   if {mechanicsmachinelocale} is "cs" or "cz":
    replace all "<prefix>" with "%{mechprefix}%" in {messagecz.foodgenerator.off}
    replace all "<foodgeneratorname>" with "%{foodgeneratorname}%" in {messageen.foodgenerator.off}
    send message "%{messageen.foodgenerator.off}%" to player
    set {fgon.%location of block%} to false
   else:
    replace all "<prefix>" with "%{mechprefix}%" in {messageen.foodgenerator.off}
    replace all "<foodgeneratorname>" with "%{foodgeneratorname}%" in {messageen.foodgenerator.off}
    send message "%{messageen.foodgenerator.off}%" to player
    set {fgon.%location of block%} to false
    
 

