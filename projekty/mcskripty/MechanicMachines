 trigger:
  if player has permission "MechanicMachines.give.foodgenerator":
   if {mechanicsmachinelocale} is "cz" or "cs":
    replace all "<foodgeneratorname>" with "%{foodgeneratorname}%" in {messagecz.foodgenerator.gived}
    replace all "<prefix>" with "%{mechprefix}%" in {messagecz.foodgenerator.gived}
    send message "%{messagecz.foodgenerator.gived}%" to player
    execute console command "give %player% 133 1 name:%{foodgeneratorname}% lore:%{foodgeneratorlore}%"
   else:
    replace all "<foodgeneratorname>" with "%{foodgeneratorname}%" in {messageen.foodgenerator.gived}
    replace all "<prefix>" with "%{mechprefix}%" in {messageen.foodgenerator.gived}
    send message "%{messageen.foodgenerator.gived}%" to player  
    execute console command "give %player% 133 1 name:%{foodgeneratorname}% lore:%{foodgeneratorlore}%"
  else:
   if {mechanicsmachinelocale} is "cz" or "cs":
    replace all "<foodgeneratorname>" with "%{foodgeneratorname}%" in {messagecz.foodgenerator.givenoperms}
    replace all "<prefix>" with "%{mechprefix}%" in {messagecz.foodgenerator.givenoperms}
    send message "%{messagecz.foodgenerator.givenoperms}%"
   else:
    replace all "<foodgeneratorname>" with "%{foodgeneratorname}%" in {messageen.foodgenerator.givenoperms}
    replace all "<prefix>" with "%{mechprefix}%" in {messageen.foodgenerator.givenoperms}
    send message "%{messageen.foodgenerator.givenoperms}%"
 
   

  
