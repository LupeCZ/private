command /giveautominer [<player>]:
 trigger: 
  if player has permissions "MechanicMachines.give.autominer":
   if {mechanicsmachinelocale} is "cz" or "cs":
    replace all "<autominername>" with "%{autominername}%" in {messagecz.autominer.gived}
    replace all "<prefix>" with "%{mechprefix}%" in {messagecz.autominer.gived}
    send message "%{messagecz.autominer.gived}%" to player
    execute console command "give %player% dispenser 1 name:%{autominername}% lore:%{autominerlore}%"
   else:
    replace all "<autominername>" with "%{autominername}%" in {messageen.autominer.gived}
    replace all "<prefix>" with "%{mechprefix}%" in {messageen.autominer.gived}
    send message "%{messageen.autominer.gived}%" to player  
    execute console command "give %player% dispenser 1 name:%{autominername}% lore:%{autominerlore}%"
  else:
   if {mechanicsmachinelocale} is "cz" or "cs":
    replace all "<autominername>" with "%{autominername}%" in {messagecz.autominer.givenoperms}
    replace all "<prefix>" with "%{mechprefix}%" in {messagecz.autominer.givenoperms}
    send message "%{messagecz.autominer.givenoperms}%"
   else:
    replace all "<autominername>" with "%{autominername}%" in {messageen.autominer.givenoperms}
    replace all "<prefix>" with "%{mechprefix}%" in {messageen.autominer.givenoperms}
    send message "%{messageen.autominer.givenoperms}%"

  
on place dispenser:
 {automineruse} is "true"
 name of player's tool is "%{autominername}%"
 set {autominer.%location of block%} to true
 if {mechanicsmachinelocale} is "cz" or "cs":
  replace all "<autominername>" with "%{autominername}%" in {messagecz.autominer.place}
  replace all "<prefix>" with "%{mechprefix}%" in {messagecz.autominer.place}
  send message "%{messagecz.autominer.place}%" to player
 else:
  replace all "<autominername>" with "%{autominername}%" in {messageen.autominer.place}
  replace all "<prefix>" with "%{mechprefix}%" in {messageen.autominer.place}
  send message "%{messageen.autominer.place}%" to player
 set {autominer.%location of block%} to true
 if {autominerPlacelogall} is "true":
  if file "plugins/Mechanic Machines/logs/Autominer/Place/all.txt" doesn't exist:
   create file "plugins/Mechanic Machines/logs/Autominer/Place/all.txt"
   set {countfile} to line count of file "plugins/Mechanic Machines/logs/Autominer/Place/all.txt"
   write "[%now%] %player% placed Autominer at %location of player% in world %world%" at line {countfile}+1 to file "plugins/Mechanic Machines/logs/Autominer/Place/all.txt"
 if {autominerPlacelogplayer} is "true":
  if file "plugins/Mechanic Machines/logs/Autominer/Place/Players/%player%.txt" doesn't exist:
   set {countfile} to line count of file "plugins/Mechanic Machines/logs/Autominer/Place/Players/%player%.txt"
   create file "plugins/Mechanic Machines/logs/Autominer/Place/Players/%player%.txt"
   write "[%now%] %player% placed Autominer at %location of player% in world %world%" at line {countfile}+1 to file "plugins/Mechanic Machines/logs/Autominer/Place/Players/%player%.txt"
 
on rightclick on dispenser:
 {automineruse} is "true"
 if {autominer.%location of clicked block%} is true:
  if {locationfo.%player%} is set:
   cancel the event
   loop blocks in radius {autominerradius} around the clicked block:
    if loop-block is any ore:
     add loop-block to inventory of block at {locationfo.%player%}
     set loop-block to air
     set {autominerlastlocale} to location of block 
   if {mechanicsmachinelocale} is "cs" or "cz":
    replace all "<prefix>" with "%{mechprefix}%" in {messagecz.autominer.activated}
    replace all "<autominername>" with "%{autominername}%" in {messagecz.autominer.activated}
    send message "%{messagecz.autominer.activated}%"
   else:
    replace all "<prefix>" with "%{mechprefix}%" in {messageen.autominer.activated}
    replace all "<autominername>" with "%{autominername}%" in {messageen.autominer.activated}
    send message "%{messageen.autominer.activated}%"   
  else:
   send message "%{messageen.autominer.salvagenotset}%"
  
	
   
command /autominer [<text>] [<text>]:
 trigger:
  if argument-text-1 is "set":
   if argument-text-2 is "salvage":
    if player has permission "MechanicMachines.set.salvage":
     if {mechanicsmachinelocale} is "cs" or "cz":
      replace all "<prefix>" with "%{mechprefix}%" in {messagecz.autominer.setsalvage}
      replace all "<autominername>" with "%{autominername}%" in {messagecz.autominer.setsalvage}
      send message "%{messagecz.autominer.setsalvage}%"
      set {collectoptions.%player%} to true
     else:
      replace all "<prefix>" with "%{mechprefix}%" in {messageen.autominer.setsalvage}
      replace all "<autominername>" with "%{autominername}%" in {messageen.autominer.setsalvage}
      send message "%{messageen.autominer.setsalvage}%"
      set {collectoptions.%player%} to true
    else:
     if {mechanicsmachinelocale} is "cs" or "cz":
      replace all "<prefix>" with "%{mechprefix}%" in {messagecz.autominer.salvage.setnoperms}
      replace all "<autominername>" with "%{autominername}%" in {messagecz.autominer.salvage.setnoperms}
      send message "%{messagecz.autominer.salvage.setnoperms}%"
     else:
      replace all "<prefix>" with "%{mechprefix}%" in {messageen.autominer.salvage.setnoperms}
      replace all "<autominername>" with "%{autominername}%" in {messageen.autominer.salvage.setnoperms}
      send message "%{messageen.autominer.salvage.setnoperms}%"	 
  if argument-text-1 is "reset":
   if argument-text-2 is "salvage":
    if player has permissions "MechanicMachines.reset.salvage":
     clear {collectlocationlocals::*}
     set {collectlocationtotals.%player%} to 0     
     if {mechanicsmachinelocale} is "cs" or "cz":
      replace all "<prefix>" with "%{mechprefix}%" in {messagecz.autominer.salvagereseted}
      replace all "<tamername>" with "%{tamername}%" in {messagecz.autominer.salvagereseted}
      send message "%{messagecz.autominer.salvagereseted}%" 
     else:
      replace all "<prefix>" with "%{mechprefix}%" in {messageen.autominer.salvagereseted}
      replace all "<tamername>" with "%{tamername}%" in {messageen.autominer.salvagereseted}
      send message "%{messageen.autominer.salvagereseted}%"
    else: 
     if {mechanicsmachinelocale} is "cs" or "cz":	 
      replace all "<prefix>" with "%{mechprefix}%" in {messagecz.autominer.salvage.resetnoperms}
      replace all "<tamername>" with "%{tamername}%" in {messagecz.autominer.salvage.resetnoperms}
      send message "%{messagecz.autominer.salvage.resetnoperms}%"
     else:
      replace all "<prefix>" with "%{mechprefix}%" in {messageen.autominer.salvage.resetnoperms}
      replace all "<tamername>" with "%{tamername}%" in {messageen.autominer.salvage.resetnoperms}
      send message "%{messageen.autominer.salvage.resetnoperms}%"	

  
on break dispenser:
 if {autominer.%location of block%} is true:
  if {mechanicsmachinelocale} is "cz" or "cs":
   replace all "<autominername>" with "%{autominername}%" in {messagecz.autominer.break}
   replace all "<prefix>" with "%{mechprefix}%" in {messagecz.autominer.break}
   send message "%{messagecz.autominer.break}%"
   execute console command "give %player% dispenser 1 name:%{autominername}% lore:%{autominerlore}%"
  else:
   replace all "<autominername>" with "%{autominername}%" in {messageen.autominer.break}
   replace all "<prefix>" with "%{mechprefix}%" in {messageen.autominer.break}
   send message "%{messageen.autominer.break}%"
   execute console command "give %player% dispenser 1 name:%{autominername}% lore:%{autominerlore}%" 
 if {autominerbreaklogall} is "true":
  if file "plugins/Mechanic Machines/logs/Autominer/Break/all.txt" doesn't exist:
   create file "plugins/Mechanic Machines/logs/Autominer/Break/all.txt"
   set {countfile} to line count of file "plugins/Mechanic Machines/logs/Autominer/Break/all.txt"
   write "[%now%] %player% broke Autominer at %location of player% in world %world%" at line {countfile}+1 to file "plugins/Mechanic Machines/logs/Autominer/Break/all.txt"
  else:
   set {countfile} to line count of file "plugins/Mechanic Machines/logs/Autominer/Break/all.txt"
   write "[%now%] %player% broke Autominer at %location of player% in world %world%" at line {countfile}+1 to file "plugins/Mechanic Machines/logs/Autominer/Break/all.txt" 
 if {autominerbreaklogplayer} is "true":
  if file "plugins/Mechanic Machines/logs/Autominer/Break/Players/%player%.txt" doesn't exist:
   set {countfile} to line count of file "plugins/Mechanic Machines/logs/Autominer/Break/Players/%player%.txt"
   create file "plugins/Mechanic Machines/logs/Autominer/Break/Players/%player%.txt"
   write "[%now%] %player% broke Autominer at %location of player% in world %world%" at line {countfile}+1 to file "plugins/Mechanic Machines/logs/Autominer/Break/Players/%player%.txt"
  else:
   set {countfile} to line count of file "plugins/Mechanic Machines/logs/Autominer/Break/Players/%player%.txt"
   write "[%now%] %player% broke Autominer at %location of player% in world %world%" at line {countfile}+1 to file "plugins/Mechanic Machines/logs/Autominer/Break/Players/%player%.txt" 
 
on rightclick:
 if clicked block is chest, trapped chest, furnace, hopper or dispenser:
  if {collectoptions.%player%} is true:
   if {collectlocationtotals.%player%} is 0:
    set {collectlocationss.%player%} to clicked block
    set {collectoptions.%player%} to false 
    add 1 to {collectlocationtotals.%player%}
    set {locationfo.%player%} to location of clicked block
    if {mechanicsmachinelocale} is "cs" or "cz":
     replace all "<prefix>" with "%{mechprefix}%" in {messagecz.autominer.salvage.was_set}
     replace all "<tamername>" with "%{tamername}%" in {messagecz.autominer.salvage.was_set}
     send message "%{messagecz.autominer.salvage.was_set}%"
    else:
     replace all "<prefix>" with "%{mechprefix}%" in {messageen.autominer.salvage.was_set}
     replace all "<tamername>" with "%{tamername}%" in {messageen.autominer.salvage.was_set}
     send message "%{messageen.autominer.salvage.was_set}%"	
   else:
    if {mechanicsmachinelocale} is "cs" or "cz":
     replace all "<prefix>" with "%{mechprefix}%" in {messagecz.autominer.salvagealreadyset}
     replace all "<autominername>" with "%{autominername}%" in {messagecz.autominer.salvagealreadyset}
     send message "%{messagecz.autominer.salvagealreadyset}%"
     set {collectoptions.%player%} to false 
    else:
     replace all "<prefix>" with "%{mechprefix}%" in {messageen.autominer.salvagealreadyset}
     replace all "<autominername>" with "%{autominername}%" in {messageen.autominer.salvagealreadyset}
     send message "%{messageen.autominer.salvagealreadyset}%"
     set {collectoptions.%player%} to false 
 if {autominersalvagelogall} is "true":
  if file "plugins/Mechanic Machines/logs/Autominer/Salvage-set/all.txt" doesn't exist:
   create file "plugins/Mechanic Machines/logs/autominer/Salvage-set/all.txt"
   set {countfile} to line count of file "plugins/Mechanic Machines/logs/autominer/Salvage-set/all.txt"
   write "[%now%] %player% set salvage at %location of player% in world %world%" at line {countfile}+1 to file "plugins/Mechanic Machines/logs/autominer/Salvage-set/all.txt"
  else:
   set {countfile} to line count of file "plugins/Mechanic Machines/logs/autominer/Salvage-set/all.txt"
   write "[%now%] %player% set salvage at %location of player% in world %world%" at line {countfile}+1 to file "plugins/Mechanic Machines/logs/autominer/Salvage-set/all.txt" 
