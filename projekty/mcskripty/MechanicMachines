on load:
 set {mechprefix} to "%yaml value ""options.Skript.prefix"" of file ""plugins/Mechanic Machines/config.yml""%"
 delete file "plugins/Mechanic Machines/updates/latestversion.yml"
 download file from "http://Mechanics.9e.cz/version.yml" to file "plugins/Mechanic Machines/updates/latestversion.yml"
 set {version} to "%yaml value ""options.Skript.version"" of file ""plugins/Mechanic Machines/config.yml""%"
 set {latestversion} to "%yaml value ""latestversion"" of file ""plugins/Mechanic Machines/updates/latestversion.yml""%"
 if {version} is {latestversion}:
  if {mechanicsmachinelocale} is "cs" or "cz":
   replace all "<prefix>" with "%{mechprefix}%" in {messagecz.Skript.updatenotavailable}
   replace all "<version>" with "%{version}%" in {messagecz.Skript.updatenotavailable}
   loop all players:
    if loop-player has permission "MechanicMachines.see.info":
     broadcast "&b&l%{messagecz.Skript.updatenotavailable}%"
  else:
   replace all "<prefix>" with "%{mechprefix}%" in {messageen.Skript.updatenotavailable}
   replace all "<version>" with "%{version}%" in {messageen.Skript.updatenotavailable}
   loop all players:
    if loop-player has permission "MechanicMachines.see.info":
     broadcast "&b&l%{messageen.Skript.updatenotavailable}%"
 else:
  if {mechanicsmachinelocale} is "cs" or "cz":
   replace all "<prefix>" with "%{mechprefix}%" in {messagecz.Skript.updateavailable}
   replace all "<version>" with "%{version}%" in {messagecz.Skript.updateavailable}  
   replace all "<latesversion>" with "%{latestversion}%" in {messagecz.Skript.updateavailable} 
   replace all "<latestversion>" with "%{latestversion}%" in {messagecz.Skript.updateavailable} 
   loop all players:
    if loop-player has permission "MechanicMachines.see.info":
     broadcast "&b&l%{messagecz.Skript.updateavailable}%"  
  else:
   replace all "<prefix>" with "%{mechprefix}%" in {messageen.Skript.updateavailable}
   replace all "<version>" with "%{version}%" in {messageen.Skript.updateavailable}    
   replace all "<latesversion>" with "%{latestversion}%" in {messageen.Skript.updateavailable}  
   replace all "<latestversion>" with "%{latestversion}%" in {messageen.Skript.updateavailable}  
   loop all players:
    if loop-player has permission "MechanicMachines.see.info":
     broadcast "&b&l%{messageen.Skript.updateavailable}%"    

on load:
 set {mechprefix} to "%yaml value ""options.Skript.prefix"" of file ""plugins/Mechanic Machines/config.yml""%"
 if {mechanicsmachinelocale} is "cs" or "cz":
  replace all "<prefix>" with "%{mechprefix}%" in {messagecz.Skript.consoleload}  
  replace all "<version>" with "%{version}%" in {messagecz.Skript.consoleload}    
  replace all "<prefix>" with "%{mechprefix}%" in {messagecz.Skript.broadcastload}  
  replace all "<version>" with "%{version}%" in {messagecz.Skript.broadcastload} 
  broadcast "%{messagecz.Skript.broadcastload}%"
  send message "%{messagecz.Skript.consoleload}%" to console
 else:
  replace all "<prefix>" with "%{mechprefix}%" in {messageen.Skript.broadcastload}  
  replace all "<version>" with "%{version}%" in {messageen.Skript.broadcastload}
  replace all "<prefix>" with "%{mechprefix}%" in {messageen.Skript.consoleload}  
  replace all "<version>" with "%{version}%" in {messageen.Skript.consoleload}
  broadcast "%{messageen.Skript.broadcastload}%"
  send message "%{messageen.Skript.consoleload}%" to console
