command /givebb [<player>]:
        trigger:
                if player has permission "MechanicMachines.give.breaker":
                        if {mechanicsmachinelocale} is "cz" or "cs":
                                replace all "<breakername>" with "%{breakername}%" in {messagecz.breaker.gived}
                                replace all "<prefix>" with "%{mechprefix}%" in {messagecz.breaker.gived}
                                send message "%{messagecz.breaker.gived}%"
                                execute console command "give %player% 23 1 name:%{breakername}% lore:%{breakerlore}%"
                        else:
                                replace all "<breakername>" with "%{breakername}%" in {messageen.breaker.gived}
                                replace all "<prefix>" with "%{mechprefix}%" in {messageen.breaker.gived}
                                send message "%{messageen.breaker.gived}%"
                                execute console command "give %player% 23 1 name:%{breakername}% lore:%{breakerlore}%"
                else:
                        if {mechanicsmachinelocale} is "cz" or "cs":
                                replace all "<foodgeneratorname>" with "%{foodgeneratorname}%" in {messagecz.breaker.givenoperms}
                                replace all "<prefix>" with "%{mechprefix}%" in {messagecz.breaker.givenoperms}
                                send message "%{messagecz.breaker.givenoperms}%"
                        else:
                                replace all "<foodgeneratorname>" with "%{foodgeneratorname}%" in {messageen.breaker.givenoperms}
                                replace all "<prefix>" with "%{mechprefix}%" in {messageen.breaker.givenoperms}
                                send message "%{messageen.breaker.givenoperms}%"

on rightclick on dispenser:
 {breakeruse} is "true"
 {bb.%location of clicked block%} is true
 cancel the event 
 if {bbon.%location of block%} is true:
  if {mechanicsmachinelocale} is "cs" or "cz":
   replace all "<breakername>" with "%{breakername}%" in {messagecz.breaker.off}
   replace all "<prefix>" with "%{mechprefix}%" in {messagecz.breaker.off}
   send message "%{messagecz.breaker.off}%" to player
  else:
   replace all "<breakername>" with "%{breakername}%" in {messageen.breaker.off}
   replace all "<prefix>" with "%{mechprefix}%" in {messageen.breaker.off}
   send message "%{messageen.breaker.off}%" to player
  set {bbon.%location of block%} to false
 else:
  set {bbon.%location of block%} to true
  if {mechanicsmachinelocale} is "cs" or "cz":
   replace all "<breakername>" with "%{breakername}%" in {messagecz.breaker.on}
   replace all "<prefix>" with "%{mechprefix}%" in {messagecz.breaker.on}
   send message "%{messagecz.breaker.on}%" to player
  else:
   replace all "<breakername>" with "%{breakername}%" in {messageen.breaker.on}
   replace all "<prefix>" with "%{mechprefix}%" in {messageen.breaker.on}
   send message "%{messageen.breaker.on}%" to player
  if block over the clicked block is chest:
   if {bbon.%location of clicked block%} is true:
    loop 1 times:
     loop {breakermaxblockss} times:
      if {bbon.%location of clicked block%} is true:
       wait 2 seconds
       clicked block is set
       add block under the clicked block to inventory of block over clicked block
       set block under the clicked block to air
      else:
       exit loop
variables:
 {bb.%block%} = false
 
on break a dispenser:
 {breakeruse} is "true"
 {bb.%location of block%} is true
 if {mechanicsmachinelocale} is "cs" or "cz":
  replace all "<breakername>" with "%{breakername}%" in {messagecz.breaker.break}
  replace all "<prefix>" with "%{mechprefix}%" in {messagecz.breaker.break}
  send message "%{messagecz.breaker.break}%" to player
 else:
  replace all "<breakername>" with "%{breakername}%" in {messageen.breaker.break}
  replace all "<prefix>" with "%{mechprefix}%" in {messageen.breaker.break}
  send message "%{messageen.breaker.break}%" to player
 cancel the event
 set event-block to air
 execute console command "give %player% 23 1 name:%{breakername}% lore:%{breakerlore}%"
 set {bb.%location of block%} to false
 set {bbon.%location of block%} to false
 if {BreakerBreaklogall} is "true":
  if "plugins/Mechanic Machines/logs/Breaker/Break/all.txt" doesn't exist:
   create file "plugins/Mechanic Machines/logs/Breaker/Break/all.txt"
   set {countfile} to line count of file "plugins/Mechanic Machines/logs/Breaker/Break/all.txt"
   write "[%now%] %player% broke Breaker at %location of player% in world %world%" at line {countfile}+1 to file "plugins/Mechanic Machines/logs/Breaker/Break/all.txt"
  else:
   write "[%now%] %player% broke Breaker at %location of player% in world %world%" at line {countfile}+1 to file "plugins/Mechanic Machines/logs/Breaker/Break/all.txt"
 if {BreakerBreaklogplayer} is "true":
  if file "plugins/Mechanic Machines/logs/Breaker/Break/Players/%player%.txt" doesn't exist:
   create file "plugins/Mechanic Machines/logs/Breaker/Break/Players/%player%.txt"
   write "[%now%] %player% placed Breaker at %location of player% in world %world%" at line {countfile}+1 to file "plugins/Mechanic Machines/logs/Breaker/Break/Players/%player%.txt"
  else:
   set {countfile} to line count of file "plugins/Mechanic Machines/logs/Breaker/Break/all.txt"
   write "[%now%] %player% placed Breaker at %location of player% in world %world%" at line {countfile}+1 to file "plugins/Mechanic Machines/logs/Breaker/Break/Players/%player%.txt"
 
on break iron block:
 {woodcutteruse} is "true"
 {woodcutter.%location of block%} is true
 if {mechanicsmachinelocale} is "cs" or "cz":
  replace all "<woodcuttername>" with "%{woodcuttername}%" in {messagecz.woodcutter.break}
  replace all "<prefix>" with "%{mechprefix}%" in {messagecz.woodcutter.break}
  send message "%{messagecz.woodcutter.break}%" to player
 else:
  replace all "<woodcuttername>" with "%{woodcuttername}%" in {messageen.woodcutter.break}
  replace all "<prefix>" with "%{mechprefix}%" in {messageen.woodcutter.break}
  send message "%{messageen.woodcutter.break}%" to player
 cancel the event
 set event-block to air
 execute console command "give %player% 23 1 name:%{woodcuttername}% lore:%{woodcutterlore}%"
 set {bb.%location of block%} to false
 set {bbon.%location of block%} to false
 if {woodcutterBreaklogall} is "true":
  if file "plugins/Mechanic Machines/logs/woodcutter/Break/all.txt" doesn't exist:
   create file "plugins/Mechanic Machines/logs/woodcutter/Break/all.txt"
   write "[%now%] %player% broke woodcutter at %location of player% in world %world%" at line {countfile}+1 to file "plugins/Mechanic Machines/logs/woodcutter/Break/all.txt"
  else:
   write "[%now%] %player% broke woodcutter at %location of player% in world %world%" at line {countfile}+1 to file "plugins/Mechanic Machines/logs/woodcutter/Break/all.txt"
 if {woodcutterBreaklogplayer} is "true":
  if file "plugins/Mechanic Machines/logs/woodcutter/Break/Players/%player%.txt" doesn't exist:
   create file "plugins/Mechanic Machines/logs/woodcutter/Break/Players/%player%.txt"
   set {countfile} to line count of file "plugins/Mechanic Machines/logs/Breaker/Break/all.txt"
   write "[%now%] %player% placed woodcutter at %location of player% in world %world%" at line {countfile}+1 to file "plugins/Mechanic Machines/logs/woodcutter/Break/Players/%player%.txt"
  else:
   set {countfile} to line count of file "plugins/Mechanic Machines/logs/Breaker/Break/all.txt"
